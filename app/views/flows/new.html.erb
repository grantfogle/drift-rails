<h1>New Stream Flow</h1>

<%= form_with model: @flow do |form| %>
    <div>
        <%= form.label :name %><br>
        <%= form.text_field :name %>
    </div>
    <div>
        <%= form.label :location %><br>
        <%= form.text_field :location %>
    </div>
    <div>
        <%= form.label :flow %><br>
        <%= form.number_field :flow %>
    </div>
    <div>
        <%= form.submit %>
    </div>
<% end %>